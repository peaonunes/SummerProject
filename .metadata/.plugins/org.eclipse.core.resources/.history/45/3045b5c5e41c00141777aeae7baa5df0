/**
 * 
 */
package BasedEntities;

/**
 * @author peaonunes
 *
 */
public class Category {
	
	private int ID;
	private String text;

	public Category(String text, int ID) {
		super();
		this.ID = ID;
		this.text = text;
	}
		
	public int getID() {
		return ID;
	}

	public void setID(int iD) {
		ID = iD;
	}

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}

}
